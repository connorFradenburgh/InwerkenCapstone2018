{
	"_version": "1.8.0",
	"sap.app": {
		"_version": "1.3.0",
		"id": "com.sap.build.standard.inwerkenPrototype",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.2.2"
		},
		"ach": "ach",
		"resources": "resources.json",
		"dataSources": {
			"local": {
				"uri": "/localService/MachineSet.json",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "/localService/metadata.xml"
				}
			},
			"Z_NDSU_COCKPIT_SRV": {
				"uri": "",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "/localService/metadata.xml"
				}
			}
		},
		"sourceTemplate": {
			"id": "sap.ui.ui5-template-plugin.2masterdetail",
			"version": "1.32.5"
		}
	},
	"sap.fiori": {
		"_version": "1.1.0",
		"registrationIds": [],
		"archeType": "transactional"
	},
	"sap.ui": {
		"_version": "1.3.0",
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://detail-view",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"_version": "1.3.0",
		"rootView": {
			"viewName": "com.sap.build.standard.inwerkenPrototype.view.App",
			"type": "XML",
			"async": false,
			"id": "app"
		},
		"autoPrefixId": true,
		"dependencies": {
			"minUI5Version": "1.50.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties"
			},
			"machineView": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "localService/mockdata/MachineSet.json"
			},
			"readingPointView": {
				"dataSource": "local",
				"type": "sap.ui.model.json.JSONModel",
				"uri": "localService/mockdata/readingPointSet.json"
			},
    		"Model": {
        		"dataSource": "local",
        		"type": "sap.ui.model.odata.v2.ODataModel",
        		"preload": true,
    			"settings": {
        			"loadMetadataAsync": false,
        			"json": true,
        			"bJSON": true,
        			"defaultBindingMode": "TwoWay",
        			"useBatch": true,
        			"refreshAfterChange": false,
        			"disableHeadRequestForToken": true
        			}
    		}

		},
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "com.sap.build.standard.inwerkenPrototype.view",
        "controlId": "idAppControl",
        "clearTarget": false,
        "controlAggregation": "detailPages",
        "async": false,
        "bypassed": {
          "target": [
            "MachineDetailPage",
            "ListOfMachines"
          ]
        }
      },
      "targets": {
        "ListOfMachines": {
          "controlAggregation": "masterPages",
          "viewName": "ListOfMachines",
          "viewId": "ListOfMachines",
          "viewLevel": 1,
          "transition": "show"
        },
        "MachineDetailPage": {
          "controlAggregation": "detailPages",
          "viewName": "MachineDetailPage",
          "viewId": "MachineDetailPage",
          "viewLevel": 1,
          "transition": "show"
        },
        "ListOfOrders": {
          "controlAggregation": "masterPages",
          "viewName": "ListOfOrders",
          "viewId": "ListOfOrders",
          "viewLevel": 1,
          "transition": "show"
        },
        "OrderDetailPage": {
          "controlAggregation": "detailPages",
          "viewName": "OrderDetailPage",
          "viewId": "OrderDetailPage",
          "viewLevel": 1,
          "transition": "show"
        }
      },
      "routes": [
        {
          "pattern": "ListOfMachines",
          "name": "ListOfMachines",
          "target": [
            "MachineDetailPage",
            "ListOfMachines"
          ]
        },
        {
          "pattern": "ListOfMachines/MachineDetailPage/:context:",
          "name": "MachineDetailPage",
          "target": [
            "ListOfMachines",
            "MachineDetailPage"
          ]
        },
        {
          "pattern": "ListOfOrders",
          "name": "ListOfOrders",
          "target": [
            "OrderDetailPage",
            "ListOfOrders"
          ]
        },
        {
          "pattern": "ListOfOrders/OrderDetailPage/:context:",
          "name": "OrderDetailPage",
          "target": [
            "ListOfOrders",
            "OrderDetailPage"
          ]
        },
        {
          "pattern": "",
          "name": "default",
          "target": [
            "MachineDetailPage",
            "ListOfMachines"
          ]
        }
      ]
    }
	}
}