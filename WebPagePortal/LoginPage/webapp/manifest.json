{
	"_version": "1.8.0",
	"sap.app": {
		"_version": "1.3.0",
		"id": "LoginPageLoginPage",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.2.2"
		},
		"ach": "ach",
		"resources": "resources.json",
		"dataSources": {
			"local": {
				"uri": "/sap/opu/odata/sap/Z_BDSY_COCKPIT_SRV",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "/localService/metadata.xml"
				}
			},
			"Z_NDSU_COCKPIT_SRV": {
				"uri": "",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "/localService/metadata.xml"
				}
			}
		},
		"sourceTemplate": {
			"id": "sap.ui.ui5-template-plugin.2masterdetail",
			"version": "1.32.5"
		}
	},
	"sap.fiori": {
		"_version": "1.1.0",
		"registrationIds": [],
		"archeType": "transactional"
	},
	"sap.ui": {
		"_version": "1.3.0",
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://detail-view",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"_version": "1.3.0",
		"rootView": {
			"viewName": "LoginPageLoginPage.view.Login",
			"type": "XML"
		},
		"autoPrefixId": true,
		"dependencies": {
			"minUI5Version": "1.50.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties"
			},
			"machineView": {
				"dataSource": "local",
				"type": "sap.ui.model.json.JSONModel",
				"uri": "localService/mockdata/MachineSet.json"
			},
			"readingPointView": {
				"dataSource": "local",
				"type": "sap.ui.model.json.JSONModel",
				"uri": "localService/mockdata/readingPointSet.json"
			},
			"orderView":{
				"dataSource": "local",
				"type": "sap.ui.model.json.JSONModel",
				"uri": "localService/mockdata/OrderSet.json"
			},
    		"": {
        		"dataSource": "local",
        		"type": "sap.ui.model.odata.v2.ODataModel",
        		"preload": true,
    			"settings": {
        			"loadMetadataAsync": false,
        			"json": true,
        			"bJSON": true,
        			"defaultBindingMode": "TwoWay",
        			"useBatch": true,
        			"refreshAfterChange": false,
        			"disableHeadRequestForToken": true
        			}
    		}

		},
    "routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "LoginPageLoginPage.view",
				
				"controlAggregation": "pages"
				
			},
			"targets": {
				"Login": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "Login",
					"controlAggregation": "pages"
				},
				"Page1": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "Page1",
					"controlId": "Login",
					"controlAggregation": "pages"
				},
				"ListOfMachines": {
					"viewType": "XML",
					"transition": "show",
					"viewName": "ListOfMachines",
					
					"viewLevel": 1
				},
				"ListOfOrders": {
					"viewType": "XML",
					"transition": "show",
					"viewName": "ListOfOrders",
					"controlAggregation": "pages",
					"viewLevel": 1
				},
				"MachineDetailPage": {
					"viewType": "XML",
					"transition": "show",
					"viewName": "MachineDetailPage",
					
					"viewLevel": 2
				},
				"OrderDetailPage": {
					"viewType": "XML",
					"transition": "show",
					"viewName": "OrderDetailPage",
					"controlAggregation": "pages",
					"viewLevel": 2
				},
				"ViewSettingsDialog1.fragment": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "ViewSettingsDialog1",
					"controlAggregation": "pages"
				},
				"ViewSettingsDialog2.fragment": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "ViewSettingsDialog2",
					"controlAggregation": "pages"
				}
			},
			"routes": [{
				"name": "Login",
				"target": ["Login"],
				"controlId": "Login"
			}, {
				"name": "Page1",
				"pattern": "Page1",
				"target": ["Page1"],
				"controlId": "Login"
			}, 
			{
				"pattern": "Split",
				"name": "Splitapp",
				"view": "App",
				"targetAggregation": "pages",
				"controlId": "Login",
				
				"subroutes":[{
					"pattern": "Split",
					"name": "ListOfMachines",
					"view": "ListOfMachines",
					"targetAggregation": "masterPages",
					"targetControl": "idAppControl",
					
					"subroutes":[{
						"pattern": "Split/{MachinePath}",
						"name": "MachineDetailPage",
						"view": "MachineDetailPage",
						"targetAggregation": "detailPages"

					}]
				}]
				
			},
			{
				"pattern": "orderSplit",
				"name": "orderSplit",
				"view": "App",
				"targetAggregation": "pages",
				"controlId": "Login",
				
				"subroutes":[{
					"pattern": "orderSplit",
					"name": "ListOfOrders",
					"view": "ListOfOrders",
					"targetAggregation": "masterPages",
					"targetControl": "idAppControl",
					
					"subroutes":[{
						"pattern": "orderSplit/{OrderPath}",
						"name": "OrderDetailPage",
						"view": "OrderDetailPage",
						"targetAggregation": "detailPages"
						
						
					}]
				}]
			},
			
			 {
				"name": "ViewSettingsDialog1",
				"pattern": "ViewSettingsDialog1",
				"controlId": "Login",
				"target": ["ViewSettingsDialog1"]
			},
			{
				"name": "ViewSettingsDialog2",
				"pattern": "ViewSettingsDialog2",
				"controlId": "Login",
				"target": ["ViewSettingsDialog2"]
			}
			]
			
	}
}
}